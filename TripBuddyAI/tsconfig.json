{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx", "global.d.ts"],
  "exclude": [
    "backend",
    "node_modules",
    "dist",
    "build"
  ]
}
